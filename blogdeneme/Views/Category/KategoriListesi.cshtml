@model IEnumerable<blogdeneme.Models.Category>


<div class="well">
    <h4>Blog Kategorileri</h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-6">
                <ul>
                    <li>
                        <a href="/Blog/List/@item.Id">@item.KategoriAdi</a>
                    </li>
                </ul>
            </div>
        }


    </div>
</div>